{
  "name": "buk-eventbus-navigator",
  "displayName": "Buk EventBus Navigator",
  "description": "CodeLens to jump from broadcast_success/workflow_event_success to EventBus YAML config and handlers.",
  "version": "0.0.1",
  "publisher": "buk",
  "repository": {
    "type": "git",
    "url": "https://github.com/vjlira/event-bus-navigator.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "contributes": {
    "commands": [
      {
        "command": "bukEventBus.openEventConfig",
        "title": "Buk EventBus: Open Event Config"
      },
      {
        "command": "bukEventBus.openHandlers",
        "title": "Buk EventBus: Open Handlers"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.19.0",
    "@types/vscode": "^1.84.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}